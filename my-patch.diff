+body {
+  font-family: -apple-system, BlinkMacSystemFont, "Segoe UI", Roboto, Helvetica, Arial, sans-serif, "Apple Color Emoji", "Segoe UI Emoji", "Segoe UI Symbol";
+  padding: 1em;
+  line-height: 1.5;
+  background-image: url('/static/pagina/fondo.png');
+  background-repeat: repeat-y;
+  background-position: center;
+  background-color: #D8E5E4;
+}
+
+.texto-justificado {
+  text-align: justify;
+}
+
+.caracteristica {
+  display: block;
+  margin: auto;
+  border:2px solid silver;
+  padding: 4px;
+}
+
+.hero {
+  margin: auto;
+  text-align: center;
+  background-color: #bbb;
+}
+
+.botones {
+  text-align: center;
+}
+
+.tc {
+  text-align: center;
+}
+
+.tl {
+  text-align: left;
+}
+
+.measure {
+  max-width: 720px;
+  margin: auto;
+}
+
+.grid {
+  display: grid;
+  grid-template-columns: repeat(3, 1fr);
+  gap: 30px;
+}
+
+.boton {
+  display: inline-block;
+  background-color: #839dcf;
+  text-decoration: none;
+  padding: 10px;
+  border-radius: 8px;
+  color: white;
+  width: 200px;
+}
+
+.boton:hover {
+  outline: 2px solid #6174ff;
+}
+
+.captura {
+  width: 256px;
+  height: 256px;
+  display: block;
+  background-color: silver;
+  image-rendering: pixelated;
+  margin: auto;
+}
+
+pre {
+  border: 1px solid #bbb;
+  padding: 1em;
+  background-color: #eee;
+  border-radius: 5px;
+}
+
